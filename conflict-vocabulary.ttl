#################################################################
# Core terminology (documentation & annotation)
# Not direct mappings of a dataset column; meant to make the KG
# self-explanatory and robust across multiple game types.
#################################################################

:CoreTerminologyScheme
  a skos:ConceptScheme ;
  skos:prefLabel "Core terminology"@en ;
  skos:definition "Core concepts used to describe the conflict game study design (game, round, decision, interaction, observability cues, and measurement terminology)."@en ;
  skos:scopeNote "Documentation scheme: not a direct mapping target for a specific dataset column."@en ;
  skos:inScheme :ConflictVocabulary ;
  skos:topConceptOf :ConflictVocabulary .

#################################################################
# Study design anchor concepts
#################################################################

:StudyDesignConcept
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Study design"@en ;
  skos:definition "Terminology used to describe the structure of an experimental study and its repeated tasks."@en .

:GameConcept
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Game"@en ;
  skos:definition "An experimental task with a defined decision problem and payoff structure."@en ;
  skos:broader :StudyDesignConcept .

:RoundConcept
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Round"@en ;
  skos:altLabel "Trial"@en ;
  skos:definition "A single repeated instance of the task in which the participant faces one opponent context and produces one decision."@en ;
  skos:broader :StudyDesignConcept .

:DecisionConcept
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Decision"@en ;
  skos:definition "A single choice made by a participant in a given round (e.g., an investment or contribution amount)."@en ;
  skos:broader :StudyDesignConcept .

:InteractionConcept
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Interaction"@en ;
  skos:definition "The contextual pairing between a focal participant and an opponent in a given round, defined by the observable information about the opponent."@en ;
  skos:broader :StudyDesignConcept .

#################################################################
# Game type hierarchy (Game -> Game type -> specific game types)
#################################################################

:GameTypeConcept
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Game type"@en ;
  skos:definition "A label identifying which experimental game/task is being played."@en ;
  skos:scopeNote "Useful when multiple games or task variants appear across datasets."@en ;
  skos:broader :GameConcept .

:AttackerDefenderContest
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Attacker–Defender contest"@en ;
  skos:altLabel "Attacker–defender game"@en ;
  skos:altLabel "AD-C"@en ;
  skos:definition "A conflict task structured around attacker and defender roles, where participants allocate part of an endowment as an investment and outcomes depend on the comparison of investments."@en ;
  skos:scopeNote "Describes the task underlying d_att_def_within and investment variables in the original dataset."@en ;
  skos:broader :GameTypeConcept .

:ContestGame
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Contest game"@en ;
  skos:altLabel "Competition game"@en ;
  skos:definition "A competitive game in which participants choose an investment level and payoffs depend on relative investments in a competitive setting."@en ;
  skos:scopeNote "For dataset1, map the behavioral choice (CGB) to conflict:d_investment on the corresponding Decision."@en ;
  skos:broader :GameTypeConcept .

:StepLevelPublicGoodsGame
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Step-level public goods game"@en ;
  skos:altLabel "Step-level PGG"@en ;
  skos:altLabel "Threshold public good game"@en ;
  skos:definition "A public goods game with a threshold (step-level) structure, where participants choose a contribution level and outcomes depend on whether the group reaches the threshold."@en ;
  skos:scopeNote "For dataset1, map the behavioral choice (SLB) to conflict:d_investment on the corresponding Decision."@en ;
  skos:broader :GameTypeConcept .

#################################################################
# Decision content terminology (what a 'decision value' means)
#################################################################

:DecisionContentConcept
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Decision content"@en ;
  skos:definition "Terminology describing what the numeric decision value represents (e.g., investment, contribution)."@en ;
  skos:broader :DecisionConcept .

:InvestmentConcept
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Investment"@en ;
  skos:definition "The amount of endowment allocated by the participant in a given round as their conflict decision."@en ;
  skos:scopeNote "Even when datasets vary in naming (e.g., contribution), d_investment is used as the generic decision amount in the KG."@en ;
  skos:broader :DecisionContentConcept .

:ContributionConcept
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Contribution"@en ;
  skos:definition "A participant's chosen contribution amount to a collective or threshold-based outcome."@en ;
  skos:scopeNote "For step-level public goods games, the decision amount can be interpreted as a contribution; the KG may still store the numeric value in d_investment."@en ;
  skos:broader :DecisionContentConcept .

#################################################################
# Interaction observability cues (what is observable about the opponent/context)
#################################################################

:ObservabilityConcept
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Opponent observability"@en ;
  skos:definition "Terminology describing what information about the opponent is observable to the participant within an interaction."@en ;
  skos:broader :InteractionConcept .

:OpponentCountryObservable
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Opponent country observable"@en ;
  skos:definition "The opponent is represented to the participant through an observable country/nationality cue, rather than through a persistent opponent identifier."@en ;
  skos:scopeNote "Supports modeling the opponent at the country level in Interaction."@en ;
  skos:broader :ObservabilityConcept .

:OpponentGroupObservable
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Opponent group relation observable"@en ;
  skos:definition "The participant is provided a cue about whether the opponent is ingroup, outgroup, or stranger."@en ;
  skos:scopeNote "Aligns with d_Ingroup_Outgroup_Stranger when that manipulation is present."@en ;
  skos:broader :ObservabilityConcept .

:OpponentRoleObservable
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Opponent role structure observable"@en ;
  skos:definition "The interaction indicates role structure (e.g., attacker vs defender) that shapes incentives and choices."@en ;
  skos:scopeNote "Aligns with the attacker/defender contest structure when present."@en ;
  skos:broader :ObservabilityConcept .

#################################################################
# Cross-scheme alignment anchors (light hierarchy)
# These help keep concept families consistent across schemes.
# They do not change dataset mappings.
#################################################################

:RoleConcept
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Role"@en ;
  skos:definition "A functional position in a decision task (e.g., attacker, defender)."@en ;
  skos:broader :DecisionConcept .

:GroupRelationConcept
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Group relation"@en ;
  skos:definition "A categorical relation between participant and opponent (ingroup/outgroup/stranger)."@en ;
  skos:broader :InteractionConcept .

# Link dataset-mapped concepts into the documentation hierarchy
:Attacker  skos:broader :RoleConcept .
:Defender  skos:broader :RoleConcept .

:Ingroup   skos:broader :GroupRelationConcept .
:Outgroup  skos:broader :GroupRelationConcept .
:Stranger  skos:broader :GroupRelationConcept .

#################################################################
# Optional measurement terminology (documentation only)
#################################################################

:MeasurementTerminologyConcept
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Measurement terminology"@en ;
  skos:definition "Terminology describing common measurement types used in behavioral game datasets (behavior vs expectation, etc.)."@en ;
  skos:broader :StudyDesignConcept .

:BehaviorMeasure
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Behavioral choice"@en ;
  skos:definition "A participant's realized choice in the game (e.g., competitive or cooperative behavior)."@en ;
  skos:scopeNote "For dataset1, CGB and SLB are behavioral measures (choices), which can be stored as d_investment in the KG."@en ;
  skos:broader :MeasurementTerminologyConcept .

:ExpectationMeasure
  a skos:Concept ;
  skos:inScheme :CoreTerminologyScheme ;
  skos:prefLabel "Expectation"@en ;
  skos:definition "A participant's stated expectation about the partner's choice in the game."@en ;
  skos:scopeNote "If expectations are not modeled in the KG, keep them out of the KG or store only as provenance metadata; avoid overloading d_investment with expectations."@en ;
  skos:broader :MeasurementTerminologyConcept .